public class Main {
    static int maxProductSubArray(int arr[]) {
        int result = Integer.MIN_VALUE;
        
        for(int i = 0; i < arr.length; i++) {  // Corrected loop condition
            for(int j = i; j < arr.length; j++) {  // Corrected inner loop
                int prod = 1;
                for(int k = i; k <= j; k++) {
                    prod *= arr[k];  // Multiply elements from i to j
                }
                result = Math.max(result, prod);  // Update the result with maximum product
            }
        }
        return result;
    }
